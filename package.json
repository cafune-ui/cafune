{
  "name": "cafune",
  "version": "1.0.0",
  "description": "A UI library deleveloped by preact",
  "main": "./cafune-ui/lib/index.js",
  "module": "./cafune-ui/components/index.js",
  "scripts": {
    "release-site": "node ./cafune-ui/scripts/release-site",
    "release": "./cafune-ui/scripts/release.sh"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evont/preact-cafune.git"
  },
  "keywords": [
    "preact"
  ],
  "author": "EvontNg <evontgoh@foxmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/evont/preact-cafune/issues"
  },
  "homepage": "https://github.com/evont/preact-cafune#readme",
  "dependencies": {
    "preact": "^8.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-angular": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "gh-pages": "^2.1.1",
    "husky": "^2.4.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
